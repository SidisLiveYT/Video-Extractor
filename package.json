{
  "name": "video-extractor",
  "version": "1.1.12",
  "description": "Video Extractor supports youtube-dl stream download for @discordjs/voice Audio Resource , for other Strean packages or functions as it will return a stream and its type",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "types": "./typings/index.d.ts",
  "files": [
    "src",
    "typings"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs"
    }
  },
  "keywords": [
    "youtube-scrapper",
    "video-extractor",
    "youtube",
    "spotify",
    "soundcloud",
    "facebook",
    "music-extractor",
    "soundcloud-scrapper",
    "spotify-scrapper",
    "youtube-extractor",
    "youtube-downloader",
    "youtube-dl-exec",
    "jericho-player"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "push": "git add . && npm run commit && git push"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "nyc": "^15.1.0",
    "semantic-release": "^18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@sidislive/youtube-dl-exec": "^1.0.0",
    "is-url": "^1.2.4",
    "play-dl": "^1.1.6",
    "spotify-url-info": "^2.2.3"
  },
  "directories": {
    "example": "example"
  },
  "author": "Sid is Live YT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SidisLiveYT/Video-Extractor.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SidisLiveYT/Video-Extractor/issues"
  },
  "homepage": "https://github.com/SidisLiveYT/Video-Extractor#readme"
}
